{
  "name": "react_redux_base",
  "version": "0.1.0",
  "description": "",
  "engines": {
    "npm": ">4",
    "node": ">5"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint:code": "esw webpack.config.* src tools --color",
    "lint:style": "stylelint \"src/styles/**/*.scss\"",
    "lint": "npm run lint:code && npm run lint:style",
    "lint:watch": "npm run lint:style && npm run lint:code -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build:dev": "npm run just-build:dev && npm run open:dist",
    "build:staging": "npm run just-build:staging && npm run open:dist",
    "build:prod": "npm run just-build:prod && npm run open:dist",
    "just-build:dev": "ENVIRONMENT=dev babel-node tools/build.js",
    "just-build:staging": "ENVIRONMENT=staging babel-node tools/build.js",
    "just-build:prod": "ENVIRONMENT=prod babel-node tools/build.js",
    "test": "jest --setupTestFrameworkScriptFile ./tools/testSetup",
    "test:CI": "node --harmony_proxies node_modules/jest/bin/jest",
    "test:cover": "npm run test -- --coverage",
    "test:cover:CI": "npm run test:cover && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch",
    "open:cover": "npm run test:cover && open coverage/lcov-report/index.html",
    "deploy:staging": "npm run just-build:staging && babel-node tools/deployS3 staging",
    "deploy:prod": "npm run just-build:prod &&  babel-node tools/deployS3 production",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js"
  },
  "author": "Bernabe Gonzalez",
  "license": "MIT",
  "dependencies": {
    "foundation-sites": "6.4.4-rc1",
    "humps": "2.0.0",
    "immutable": "3.8.1",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.17.4",
    "prop-types": "15.5.10",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-redux": "5.0.4",
    "react-router": "3.0.5",
    "react-router-redux": "4.0.8",
    "react-router-scroll": "0.4.2",
    "react-widgets": "4.1.3",
    "redux": "3.6.0",
    "redux-form": "6.7.0",
    "redux-logger": "3.0.1",
    "redux-react-session": "2.0.0",
    "redux-thunk": "2.2.0",
    "validate.js": "0.11.1"
  },
  "devDependencies": {
    "autoprefixer": "6.7.3",
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.3.2",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.5.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-register": "6.24.1",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "coveralls": "2.11.16",
    "css-loader": "0.28.1",
    "dotenv": "4.0.0",
    "dotenv-webpack": "1.5.4",
    "enzyme": "2.8.2",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "eslint-watch": "3.1.0",
    "express": "4.15.2",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "html-webpack-plugin": "2.28.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "20.0.1",
    "json-loader": "0.5.4",
    "nock": "9.0.13",
    "node-sass": "4.5.3",
    "npm-run-all": "4.0.2",
    "opn": "5.0.0",
    "postcss-loader": "1.3.3",
    "prompt": "1.0.0",
    "react-hot-loader": "3.0.0-beta.7",
    "react-test-renderer": "15.5.4",
    "redux-immutable-state-invariant": "2.0.0",
    "redux-mock-store": "1.2.1",
    "replace": "0.3.0",
    "rimraf": "2.5.4",
    "sass-loader": "6.0.5",
    "style-loader": "0.17.0",
    "stylelint": "7.10.1",
    "stylelint-config-standard": "16.0.0",
    "stylelint-order": "0.2.2",
    "url-loader": "0.5.8",
    "webpack": "2.5.1",
    "webpack-bundle-analyzer": "2.6.0",
    "webpack-dev-middleware": "1.10.2",
    "webpack-hot-middleware": "2.18.0",
    "webpack-md5-hash": "0.0.5"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|ico)$": "<rootDir>/tools/fileMock.js"
    }
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": ""
  }
}
